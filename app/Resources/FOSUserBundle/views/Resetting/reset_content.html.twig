{% trans_default_domain 'FOSUserBundle' %}
<form action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST" class="fos_user_resetting_reset smallForm">
   <h2>{{ 'security.base.reset'|trans({}, "default" )}}</h2>
    <div class="formContent">

        {{ form_widget(form) }}
        <input type="submit" value="{{ 'resetting.reset.submit'|trans }}" />
    </div>
</form>
